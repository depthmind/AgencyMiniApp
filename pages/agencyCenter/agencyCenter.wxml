      <view class="item">
        <view style="color: #f19d21;">0<text style="font-size: 24rpx;">个</text></view>
        <text>今日订单</text>
      </view>
      <view class="item">
        <view style="color: #e66a48;">0.00<text style="font-size: 24rpx;">元</text></view>
