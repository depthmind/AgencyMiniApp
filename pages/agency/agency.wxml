<view class="container">
  <view>
    <view>代理商名称</view>
    <input></input>
  </view>
  <view>
    <view>详细地址</view>
    <text bindtap='chooseAddress'>></text>
  </view>
  <view>显示区域</view>
  <!--三级选择-->
