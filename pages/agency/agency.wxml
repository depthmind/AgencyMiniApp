    <view style="width: 100px; height: 100px; background-color: #eeeeee;" bindtap='chooseImage'>
      <image style="width: 100px; height: 100px; background-color: #eeeeee;"  src="{{imagePath}}"></image>
    </view>
  </view>
</view>
