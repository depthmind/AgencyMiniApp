<view>
  <picker-view
    indicator-style="height: 50px;"
    style="width: 100%; height: 300px;"
    value="{{value}}"
    bindchange="bindChange"
  >
    <picker-view-column>
      <view wx:for="{{province}}" style="line-height: 50px">{{item.fullname}}</view>
    </picker-view-column>
    <picker-view-column>
      <view wx:for="{{city}}" style="line-height: 50px">{{item.fullname}}</view>
    </picker-view-column>
    <picker-view-column>
      <view wx:for="{{area}}" style="line-height: 50px">{{item.fullname}}</view>
    </picker-view-column>
  </picker-view>
</view>



